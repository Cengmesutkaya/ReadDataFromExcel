
@{
    ViewBag.Title = "CreateExcel";
}
<br />
<br />
<br />
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            @using (Html.BeginForm("UploadExcel", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="card-body">

                    <div class="form-group row">
                        <label for="basic-input" class="col-sm-4 col-form-label">Excel</label>
                        <div class="col-sm-10">
                            <input type="file" name="uploadfile" class="" accept=".xlsx, .xls" />
                        </div>
                    </div>
                    <div class="form-footer">
                        <button type="submit" class="btn btn-success form-control"><i class="fa fa-check-square-o"></i>Değişiklikleri Kaydet</button>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
